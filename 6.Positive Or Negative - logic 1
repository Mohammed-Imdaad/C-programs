//Positive or Negative  - Trick 1 

#include<stdio.h>
void main()
{
    int n;
    scanf("%d",&n);
    if(n==0)
    {
        printf("%d is neither positive nor negative",n);
    }
    else if(n>0)
    {
        printf("%d is positive",n);
    }
    else
    {
        printf("%d is negative",n);
    }
    
}
