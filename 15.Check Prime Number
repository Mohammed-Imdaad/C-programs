//Print given number is prime or not
//prime number - which is divible by 1 and itself.

#include<stdio.h>
void main()
{
    int n,i,c=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++) // 1 and itself logic
    {
       
        if(n%i==0)
        {
            c++;
        }
    }
    if(c==2)
    {
        printf("%d is a prime number",n);
    }
    else
    {
        printf("%d is not a prime number",n);
    }
}
